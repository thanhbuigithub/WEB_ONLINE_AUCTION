														<input type="radio" name="product_color"
															style="background:#000000">
														<input type="radio" name="product_color"
															style="background:#999999">
													</div>
													<button onclick="location.href='/account/cart/{{_id}}'"
														class="product_cart_button">Add to Cart</button>
												</div>
											</div>
											<div class="product_fav" data-id="{{_id}}"><i class="fas fa-heart"></i>
											</div>
											{{#if isNew}}
											<ul class="product_marks">
												<li class="product_mark product_discount">new</li>
											</ul>
											{{/if}}
											{{!-- <ul class="product_marks">
